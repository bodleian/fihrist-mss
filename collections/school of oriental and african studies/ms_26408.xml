                  <msItem xml:id="ms_26408-item1" n="1">
                     <author key="person_90059668" ref="http://viaf.org/viaf/90059668/">
                        <persName xml:lang="ar-Latn-x-lc" type="standard">Nawawī, 1233-1277</persName>
                        <persName xml:lang="ar">النووي، ابي زكريا يحيى ابن شرف</persName>
                     </author>
                     <title xml:lang="ar-Latn-x-lc" key="work_10492">Ḥizb al-Nawawī</title>
                     <title xml:lang="ar" key="work_10492">حزب النووي</title>
                     <incipit xml:lang="ar">بسم الله الله اكبر الله اكبر اقول على نفسى ... وعلى دينى وعلى اهلى وعلى اولادى وعلى مالى</incipit>
                     <explicit xml:lang="ar" defective="true"/>
                     <note>
                        <foreign xml:lang="ar"/>
                     </note>
                     <listBibl>
                        <bibl>
                           <ref target="http://digital.info.soas.ac.uk/15594/#page/1/mode/2up/search/">view the printed catalogue</ref>
                        </bibl>
                        <bibl>Gacek 110</bibl>
